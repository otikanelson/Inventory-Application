{
  "expo": {
    "name": "Stockq",
    "slug": "stockq",
    "version": "1.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "backgroundColor": "#000000",
    "scheme": "inventory",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos of products.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select product images.",
        "NSPhotoLibraryAddUsageDescription": "This app needs permission to save photos to your library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.son_the_nel.Inventory",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "usesCleartextTraffic": true,
      "networkSecurityConfig": "./android/network_security_config.xml"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your photo library to select product images.",
          "cameraPermission": "This app needs access to your camera to take photos of products."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7ccb323f-ce43-4d57-aaae-d99c8139faa3"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "neil2022",
    "updates": {
      "url": "https://u.expo.dev/7ccb323f-ce43-4d57-aaae-d99c8139faa3"
    }
  }
}
